{
  "name": "nestjs-handlebars",
  "version": "1.0.0",
  "description": "NestJS Handlebars module with i18n support",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prerelease": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahsumurebe/nestjs-handlebars.git"
  },
  "keywords": [
    "nestjs",
    "handlebars",
    "i18n"
  ],
  "author": "Mahsum UREBE <info@mahsumurebe.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mahsumurebe/nestjs-handlebars/issues"
  },
  "homepage": "https://github.com/mahsumurebe/nestjs-handlebars#readme",
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-angular": "19.1.0",
    "@nestjs/common": "10.3.4",
    "@nestjs/core": "10.3.4",
    "@nestjs/platform-express": "10.3.4",
    "@nestjs/testing": "10.3.4",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "20.11.29",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.3.0",
    "@typescript-eslint/parser": "7.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.4.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "dependencies": {
    "debug": "^4.3.4",
    "glob": "^10.3.10",
    "handlebars": "^4.7.8",
    "i18next": "^23.10.1",
    "lodash": "^4.17.21"
  }
}
